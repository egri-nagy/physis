physis_home=/home/sirna/physis/
max_number_of_updates=400
#random_seed=329072
random_number_generator=JavaUtilRandom

#seed organisms. Signed by integers begining from 1 
seed_organism1=data/genebank/mar/mar.short
seed_organism2=data/genebank/mar/mar.short
seed_organism3=data/genebank/mar/mar.short
seed_organism4=data/genebank/mar/mar.short
seed_organism5=data/genebank/mar/mar.short
seed_organism6=data/genebank/mar/mar.short
seed_organism7=data/genebank/mar/mar.short
seed_organism8=data/genebank/mar/mar.short
seed_organism9=data/genebank/mar/mar.short
seed_organism10=data/genebank/mar/mar.short

# virtualmachine related stuff
virtual_machine_class_name=mar.virtualmachine.MarVM

# nursing
nurse_class_name=physis.core.nursing.MaxAgePerMeritNurse

# instruction set
instructionset=data/instructionsets/mar.standard


# lifespace
lifespace_class_name=physis.core.lifespace.Lattice2DLifeSpace
2Dlattice_xsize=100
2Dlattice_ysize=100

# time_slicing
scheduler_class_name=physis.core.scheduler.ScaledMeritScheduler
average_time_slice=11

# mutation 
  # copy mutation / per copy
  copy_mutation_rate=0.01
  # mutation / per divide
  divide_mutation_rate=0.0
  divide_insert_rate=0.013
  divide_delete_rate=0.013

#input_data
input_data_higher_bound=13

# memory allocation
min_allocation_ratio=0.5
max_allocation_ratio=2

# divide ratios
min_proliferation_ratio=0.80

# genebank
gene_bank_enabled=false
expiration_time=100
fossil_time=1600
spawn_threshold=0
gene_bank_max_size=100000
gene_bank_output_file=results/genebank

# tasks
task_filename=data/tasks/standard.tasks
max_number_of_tasks=20

# statistics
statistics_configuration=data/statistics/statistics.conf
statistics_data_file=results/statistics

#triggers
trigger_file=data/triggers/extract






